{
  "name": "n8n-nodes-predictleads",
  "version": "0.1.0",
  "description": "PredictLeads community node for n8n (companies, signals, enrichment).",
  "license": "MIT",
  "homepage": "https://predictleads.com",
  "keywords": [
    "n8n-community-node-package",
    "n8n-nodes",
    "predictleads",
    "sales-intelligence",
    "enrichment"
  ],
  "author": {
    "name": "PredictLeads",
    "email": "support@predictleads.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PredictLeads/n8n-nodes-predict-leads.git"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
